{
  "name": "rhodes-headquarters",
  "displayName": "罗德岛远程指挥部",
  "type": "module",
  "version": "0.3.0",
  "publishVersion": "0.3.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "description": "一款用于监控罗德岛运行状态的浏览器扩展",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt --browser firefox",
    "build": "wxt build",
    "build:firefox": "wxt build --browser firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip --browser firefox",
    "postinstall": "wxt prepare && simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "e2e": "PW_EXPERIMENTAL_SERVICE_WORKER_NETWORK_EVENTS=1 playwright test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ark-ui/vue": "^4.5.0",
    "@types/crypto-js": "^4.2.2",
    "@unocss/preset-mini": "^0.63.6",
    "@webext-core/proxy-service": "^1.2.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "node-forge": "^1.3.1",
    "ofetch": "^1.4.1",
    "pinia": "^2.2.4",
    "ufo": "^1.5.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.23.2",
    "@iconify/json": "^2.2.263",
    "@playwright/test": "^1.48.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.9",
    "@types/node-forge": "^1.3.11",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@unocss/eslint-plugin": "^0.63.6",
    "@unocss/reset": "^0.63.6",
    "@vue/compiler-sfc": "^3.5.12",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.1.0",
    "@wxt-dev/module-vue": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unocss-preset-animations": "^1.1.0",
    "unplugin-vue-router": "^0.10.8",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^2.1.3",
    "vue": "^3.5.12",
    "vue-demi": "^0.14.10",
    "wxt": "^0.19.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ]
    },
    "patchedDependencies": {
      "vite-plugin-vue-devtools": "patches/vite-plugin-vue-devtools.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
